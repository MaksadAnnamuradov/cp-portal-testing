{
  "name": "cplite",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mdi/font": "^7.3.67",
    "appcache-webpack-plugin": "^1.4.0",
    "axios": "^0.19.2",
    "core-js": "^2.6.11",
    "csvtojson": "^2.0.10",
    "devextreme": "23.1.7",
    "devextreme-vue": "23.1.7",
    "intersection-observer": "^0.7.0",
    "material-design-icons-iconfont": "^5.0.1",
    "mobile-drag-drop": "^2.3.0-rc.2",
    "platform": "^1.3.5",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.11",
    "vue-burger-menu": "^2.0.3",
    "vue-custom-scrollbar": "^1.2.0",
    "vue-light-gallery": "^1.0.1",
    "vue-router": "^3.1.6",
    "vue-virtual-scroller": "^1.1.2",
    "vue2-editor": "^2.10.3",
    "vuetify": "^2.0.15"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-pwa": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "csv-loader": "^3.0.5",
    "deepmerge": "^4.2.2",
    "fibers": "^4.0.2",
    "raw-loader": "^4.0.2",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "to-string-loader": "^1.2.0",
    "vue-template-compiler": "^2.6.11",
    "xml-loader": "^1.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "globals": {
      "Sublib": true,
      "RestClient": true,
      "IDB": true,
      "EventBus": true,
      "android": true,
      "Offline": true,
      "$": true,
      "loadImage": true,
      "QRCode": true,
      "sqlExec": true
    },
    "rules": {
      "no-constant-condition": "off",
      "no-unused-vars": "off",
      "no-empty": "off",
      "no-debugger": "off",
      "no-useless-escape": "off",
      "nested-comment": "on",
      "no-unreachable": "off",
      "css-ruleorselectorexpected": "off",
      "vue/return-in-computed-property": "off",
      "no-console": "off",
      "vue/no-v-text-v-html-on-component": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
